{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "data/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/script.js",
      "dest": "/script-obfuscated.js"
    },
    {
      "src": "/public/kjv.json",
      "dest": "/api/blocked",
      "status": 403
    },
    {
      "src": "/([^/]+)/([^/]+)/([^/]+)",
      "dest": "/server.js"
    },
    {
      "src": "/([^/]+)/([^/]+)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
} 