{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/favicon.png",
      "dest": "/favicon.ico"
    },
    {
      "src": "/hero.png",
      "dest": "/public/hero.png"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
} 